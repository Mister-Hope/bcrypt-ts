{
  "name": "bcrypt-ts",
  "version": "5.0.2",
  "description": "bcrypt written in typescript",
  "keywords": [
    "bcrypt",
    "bcryptjs"
  ],
  "bugs": {
    "url": "https://github.com/Mister-Hope/bcrypt-ts/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Mr.Hope",
    "email": "mister-hope@outlook.com",
    "url": "https://mister-hope.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/node.d.mts",
      "browser": "./dist/browser.mjs",
      "node": "./dist/node.mjs",
      "import": "./dist/node.mjs",
      "require": "./dist/node.cjs",
      "default": "./dist/node.cjs"
    },
    "./browser": {
      "types": "./dist/browser.d.mts",
      "import": "./dist/browser.mjs",
      "require": "./dist/browser.cjs",
      "default": "./dist/browser.mjs"
    },
    "./node": {
      "types": "./dist/node.d.mts",
      "import": "./dist/node.mjs",
      "require": "./dist/node.cjs",
      "default": "./dist/node.cjs"
    }
  },
  "main": "./dist/node.cjs",
  "module": "./dist/node.mjs",
  "browser": "./dist/browser.mjs",
  "types": "./dist/node.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "changelog": " commit-and-tag-version --skip.bump --skip.commit --skip.tag",
    "clean": "rimraf dist",
    "commit": "pnpm git-cz",
    "dev": "rollup -c rollup.config.ts --configPlugin esbuild -w",
    "lint": "pnpm lint:eslint && pnpm lint:prettier",
    "lint:eslint": "eslint --fix .",
    "lint:prettier": "prettier --check --write .",
    "prepare": "husky",
    "prepublish": "pnpm clean && pnpm build",
    "test": "vitest --coverage"
  },
  "nano-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js,ts}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@rollup/plugin-alias": "5.1.1",
    "@rollup/plugin-replace": "6.0.1",
    "@types/node": "22.8.6",
    "@vitest/coverage-v8": "2.1.3",
    "commit-and-tag-version": "12.5.0",
    "cz-git": "1.10.1",
    "esbuild": "0.24.0",
    "eslint": "9.13.0",
    "eslint-config-mister-hope": "0.3.0",
    "husky": "9.1.6",
    "nano-staged": "0.8.0",
    "prettier": "3.3.3",
    "rimraf": "6.0.1",
    "rollup": "4.24.3",
    "rollup-plugin-dts": "6.1.1",
    "rollup-plugin-esbuild": "6.1.1",
    "typescript": "5.6.3",
    "vite": "5.4.10",
    "vitest": "2.1.3"
  },
  "packageManager": "pnpm@9.12.2",
  "engines": {
    "node": ">=18"
  }
}
