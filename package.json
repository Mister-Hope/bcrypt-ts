{
  "name": "bcrypt-ts",
  "version": "4.0.0",
  "description": "bcrypt written in typescript",
  "keywords": [
    "bcrypt",
    "bcryptjs"
  ],
  "bugs": {
    "url": "https://github.com/Mister-Hope/bcrypt-ts/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Mr.Hope",
    "email": "mister-hope@outlook.com",
    "url": "https://mister-hope.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/node.d.mts",
      "browser": "./dist/browser.mjs",
      "node": "./dist/node.mjs",
      "import": "./dist/node.mjs",
      "require": "./dist/node.cjs",
      "default": "./dist/node.cjs"
    },
    "./browser": {
      "types": "./dist/browser.d.mts",
      "import": "./dist/browser.mjs",
      "require": "./dist/browser.cjs",
      "default": "./dist/browser.mjs"
    },
    "./node": {
      "types": "./dist/node.d.mts",
      "import": "./dist/node.mjs",
      "require": "./dist/node.cjs",
      "default": "./dist/node.cjs"
    }
  },
  "main": "./dist/node.cjs",
  "module": "./dist/node.mjs",
  "browser": "./dist/browser.mjs",
  "types": "./dist/node.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "changelog": " commit-and-tag-version --skip.bump --skip.commit --skip.tag",
    "clean": "rimraf dist",
    "commit": "pnpm git-cz",
    "lint": "pnpm lint:eslint && pnpm lint:prettier",
    "lint:eslint": "eslint --fix --ext .js,.ts src",
    "lint:prettier": "prettier --check --write .",
    "packages:check-update": "pnpm dlx npm-check-updates -u --timeout 600000",
    "packages:update": "pnpm up",
    "prepare": "husky install",
    "test": "vitest --coverage"
  },
  "nano-staged": {
    "*.{js,ts}": "eslint --fix",
    "*.{js,ts,json,yml,md}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.2",
    "@commitlint/config-conventional": "17.7.0",
    "@rollup/plugin-alias": "5.0.1",
    "@rollup/plugin-replace": "5.0.3",
    "@types/node": "20.8.4",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "@vitest/coverage-v8": "0.34.6",
    "commit-and-tag-version": "11.3.0",
    "commitizen": "4.3.0",
    "cz-git": "1.7.1",
    "esbuild": "0.19.4",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "8.0.3",
    "nano-staged": "0.8.0",
    "prettier": "3.0.3",
    "rimraf": "5.0.5",
    "rollup": "4.0.2",
    "rollup-plugin-dts": "6.1.0",
    "rollup-plugin-esbuild": "6.1.0",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
    "vite": "4.4.11",
    "vitest": "0.34.6"
  },
  "packageManager": "pnpm@8.9.0"
}
