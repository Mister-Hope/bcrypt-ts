{
  "name": "bcrypt-ts",
  "version": "3.0.0",
  "description": "bcrypt written in typescript",
  "keywords": [
    "bcrypt",
    "bcryptjs"
  ],
  "license": "MIT",
  "author": {
    "name": "Mr.Hope",
    "email": "mister-hope@outlook.com",
    "url": "https://mrhope.site"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/node.d.mts",
      "browser": "./dist/browser.mjs",
      "node": "./dist/node.mjs",
      "import": "./dist/node.mjs",
      "require": "./dist/node.cjs",
      "default": "./dist/node.cjs"
    },
    "./browser": {
      "types": "./dist/browser.d.mts",
      "import": "./dist/browser.mjs",
      "require": "./dist/browser.cjs",
      "default": "./dist/browser.mjs"
    },
    "./node": {
      "types": "./dist/node.d.mts",
      "import": "./dist/node.mjs",
      "require": "./dist/node.cjs",
      "default": "./dist/node.cjs"
    }
  },
  "main": "./dist/node.cjs",
  "module": "./dist/node.mjs",
  "browser": "./dist/browser.mjs",
  "types": "./dist/node.d.mts",
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/Mister-Hope/bcrypt-ts/issues"
  },
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "clean": "rimraf dist",
    "commit": "pnpm git-cz",
    "changelog": "standard-version --skip.bump --skip.commit --skip.tag",
    "lint": "pnpm lint:eslint && pnpm lint:prettier",
    "lint:eslint": "eslint --ext .js,.ts src",
    "lint:prettier": "prettier --check --write .",
    "packages:check-update": "pnpm dlx npm-check-updates -u --timeout 600000",
    "packages:update": "pnpm up",
    "test": "vitest --coverage"
  },
  "nano-staged": {
    "*.{js,ts}": "eslint --fix",
    "*.{js,ts,json,yaml,yml,md}": "prettier --write",
    "*.wxss": "stylelint --fix",
    "*.wxml": "prettier --parser html --write",
    "*.wxs": "prettier --parser babel-flow --write",
    "package.json": "sort-package-json"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "packageManager": "pnpm@7.27.1",
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@rollup/plugin-alias": "4.0.3",
    "@rollup/plugin-replace": "5.0.2",
    "@types/node": "18.14.1",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "@vitest/coverage-c8": "0.29.1",
    "commitizen": "4.3.0",
    "cz-git": "1.5.0",
    "c8": "7.13.0",
    "esbuild": "0.17.10",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "nano-staged": "0.8.0",
    "prettier": "2.8.4",
    "rimraf": "4.1.2",
    "rollup": "3.17.3",
    "rollup-plugin-dts": "5.2.0",
    "rollup-plugin-esbuild": "5.0.0",
    "standard-version": "9.5.0",
    "tslib": "2.5.0",
    "typescript": "4.9.5",
    "vite": "4.1.4",
    "vitest": "0.29.1"
  }
}
