{
  "name": "bcrypt-ts",
  "version": "8.0.0",
  "description": "bcrypt written in typescript",
  "keywords": [
    "bcrypt",
    "bcryptjs"
  ],
  "bugs": {
    "url": "https://github.com/Mister-Hope/bcrypt-ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Mister-Hope/bcrypt-ts.git"
  },
  "license": "MIT",
  "author": {
    "name": "Mr.Hope",
    "email": "mister-hope@outlook.com",
    "url": "https://mister-hope.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/node.d.ts",
      "browser": "./dist/browser.js",
      "node": "./dist/node.js",
      "default": "./dist/node.js"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "default": "./dist/browser.js"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "default": "./dist/node.js"
    }
  },
  "browser": "./dist/browser.mjs",
  "types": "./dist/node.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "bench": "vitest bench",
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "clean": "rimraf dist",
    "dev": "rollup -c rollup.config.ts --configPlugin esbuild -w",
    "lint": "pnpm lint:eslint && pnpm lint:prettier",
    "lint:eslint": "eslint --fix .",
    "lint:prettier": "prettier --check --write .",
    "prepare": "husky",
    "prepublish": "pnpm clean && pnpm build",
    "test": "vitest --coverage"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "nano-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js,ts}": "eslint --fix"
  },
  "devDependencies": {
    "@codecov/rollup-plugin": "1.9.1",
    "@commitlint/cli": "20.3.1",
    "@commitlint/config-conventional": "20.3.1",
    "@rollup/plugin-alias": "6.0.0",
    "@rollup/plugin-replace": "6.0.3",
    "@types/node": "24.10.6",
    "@vitest/coverage-istanbul": "4.0.16",
    "eslint": "9.39.2",
    "eslint-config-mister-hope": "0.9.6",
    "husky": "9.1.7",
    "nano-staged": "0.9.0",
    "prettier": "3.7.4",
    "rimraf": "6.1.2",
    "rollup": "4.55.1",
    "rollup-plugin-dts": "6.3.0",
    "rollup-plugin-esbuild": "6.2.1",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  },
  "packageManager": "pnpm@10.24.0",
  "engines": {
    "node": ">=20"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "unrs-resolver"
    ]
  }
}
